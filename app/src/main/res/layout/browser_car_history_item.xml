<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/article_car_hitory_rootview"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/browser_weblable_height">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:paddingStart="6dp"
        android:layout_marginTop="2dp"
        android:paddingEnd="6dp"
        android:background="@drawable/browser_default_bg"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/car_history_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginLeft="2dp"
            android:layout_marginStart="2dp"
            android:layout_gravity="center_vertical"/>
        <TextView
            android:id="@+id/car_history_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_marginRight="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:layout_gravity="center_vertical"
            android:textColor="#FF111E36"
            android:textSize="14sp" />

    </LinearLayout>

</FrameLayout>