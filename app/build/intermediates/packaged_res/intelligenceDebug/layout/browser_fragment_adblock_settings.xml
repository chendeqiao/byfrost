<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/activity_background"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/browser_setting_item_minheight"
        android:background="@color/activity_background">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:text="@string/pref_ad_block"
            android:textColor="@color/browser_title_color"
            android:textSize="@dimen/settings_list_item_title_size" />

        <com.intelligence.browser.ui.widget.BrowserSwitchButton
            android:id="@+id/switch_button"
            style="@style/SwitchButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="30dp"
            android:layout_marginRight="20dp"
            app:browserBackDrawable="@drawable/browser_switch_track"
            app:browserBackMeasureRatio="1.6"
            app:browserThumbDrawable="@drawable/browser_switch_thumb"
            app:browserThumbHeight="@dimen/browser_switch_thumb_wh"
            app:browserThumbWidth="@dimen/browser_switch_thumb_wh" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/settings_item_background" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/activity_background">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:text="@string/pic_ad_block_count"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size" />

        <TextView
            android:id="@+id/pic_ad_block_count_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="30dp"
            android:layout_marginRight="30dp"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size"
            tools:text="45条" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/activity_background">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:text="@string/js_ad_block_count"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size" />

        <TextView
            android:id="@+id/js_ad_block_count_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="30dp"
            android:layout_marginRight="30dp"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size"
            tools:text="45条" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/activity_background">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:text="@string/popup_ad_block_count"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size" />

        <TextView
            android:id="@+id/popup_ad_block_count_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="30dp"
            android:layout_marginRight="30dp"
            android:textColor="@color/browser_tips_color"
            android:textSize="@dimen/settings_list_item_summary_size"
            tools:text="45条" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@color/activity_background">

        <TextView
            android:id="@+id/clear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:paddingTop="16dp"
            android:paddingEnd="30dp"
            android:paddingRight="30dp"
            android:text="@string/clear"
            android:textColor="@color/browser_title_blue"
            android:textSize="@dimen/settings_list_item_summary_size" />

    </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="12dp"
        android:background="@color/settings_item_background" />

    <RelativeLayout
        android:id="@+id/edit_rules_layout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/activity_background"
        android:visibility="gone">

        <TextView
            android:id="@+id/edit_rules"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:text="@string/custom_rules"
            android:textColor="@color/browser_title_color"
            android:textSize="@dimen/settings_list_item_title_size" />

    </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="3dp"
        android:background="@color/settings_item_background"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/set_js_disable_layout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/activity_background">

        <TextView
            android:id="@+id/set_js_disable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginEnd="100dp"
            android:singleLine="true"
            android:ellipsize="middle"
            android:text="@string/js_disable_hosts"
            android:textColor="@color/browser_title_color"
            android:textSize="@dimen/settings_list_item_title_size" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="28dp"
            android:gravity="center_vertical"
            android:layout_marginRight="28dp">

            <TextView
                android:id="@+id/ad_limite_time_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ad_limite_time_arrow"
                android:textColor="@color/browser_tips_color"
                android:textSize="@dimen/settings_list_item_summary_size" />

            <ImageView
                android:id="@+id/ad_limite_time_arrow"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/browser_setting_right_arrow" />
        </LinearLayout>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/settings_item_background" />

</LinearLayout>